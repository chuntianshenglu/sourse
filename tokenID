        //-- begin code for token holders sparkline graph---    
        var holdersplotData = '';
        var holdersplotMinValue = 0;
        var blnAlsoShowPriceInSpark = true;

        if (holdersplotData != '') {
            var sparkOptions = {
                chart: {
